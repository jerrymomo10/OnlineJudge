class Solution {
public:
    int singleNumber(int A[], int n) {
        int ones=0,twos=0,three=0;
        for(int i=0;i<n;i++)
        {
            twos|=ones&A[i];
            ones^=A[i];
            three=ones&twos;
            ones&=~three;
            twos&=~three;
        }
        return ones;
    }
};
