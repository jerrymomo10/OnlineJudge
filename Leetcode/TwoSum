#include <vector>
#include <iostream>
#include <map>
using namespace std;
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
		vector<int> result;
		map<int,int> t;
        for(unsigned int i=0;i<nums.size();i++)
        {
			int current = nums[i];
			int left = target-current;
			if(t.count(left)!=0)
			{
				result.push_back(t[left]+1);
				result.push_back(i+1);
				}
			t[current]=i;
		}
		return result;
	}	
};
int main()
{
	Solution s;
	vector<int> a;
	for(unsigned int i=0;i!=10;i++)
	{
		a.push_back(i);
		}
	unsigned int target = 6;
	vector<int> b = s.twoSum(a,target);
	for(unsigned int i=0;i<b.size();i++)
	{
		cout<<b[i]<<endl;
		}
	return 1;
}
