class Solution {
public:
    int numTrees(int n) {
        if(n==1)return 1;
        if(n==2)return 2;
        int t[n+1];
        t[0]=1;
        t[1]=1;
        t[2]=2;
        for(int i=3;i<n+1;i++)
        {
            int temp=0;
            for(int j=0;j<i;j++)
            {
                temp+=t[j]*t[i-j-1];
            }
            t[i]=temp;
        }
        return t[n];
    }
};
